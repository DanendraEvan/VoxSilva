{
  "name": "voxsilva-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@heroicons/react": "^2.2.0",
    "autoprefixer": "^10.4.17",
    "framer-motion": "^12.23.24",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.30.2",
    "react-scripts": "5.0.1",
    "tailwindcss": "^3.3.6",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run copy-pwa",
    "copy-pwa": "copy public\\service-worker.js build\\service-worker.js 2>nul & copy public\\js\\pwa-install.js build\\js\\pwa-install.js 2>nul || echo PWA files copied",
    "clean": "if exist build rmdir /s /q build",
    "rebuild": "npm run clean && npm run build",
    "predeploy": "npm run rebuild",
    "deploy": "firebase deploy --only hosting",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
  }
}
